<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite thread-count="5" name="Parameters Suite" parallel="classes">
    <parameter name="browserType" value="chrome"></parameter>
    <test name="Login Logout Calendar">
        <classes>
            <class name="tests.LoginLogoutTest"> </class>
            <class name="tests.CalendarTest"> </class>
        </classes>
    </test>
    <test name="Add Bikes, Shoes, Routes">
        <classes>
            <class name="tests.AddBikesTest"> </class>
            <class name="tests.AddShoesTest"> </class>
            <class name="tests.AddRoutesTest"> </class>
        </classes>
    </test>
    <test name="Customize Activity Types">
        <classes>
            <class name="tests.CustomizeActivityTypesTest"> </class>
        </classes>
    </test>
    <test name="Edit User Profile">
        <classes>
            <class name="tests.EditUserProfileTest"> </class>
        </classes>
    </test>
    <test name="Add Workout">
        <classes>
            <class name="tests.AddWorkoutTest"> </class>
        </classes>
    </test>
</suite>
